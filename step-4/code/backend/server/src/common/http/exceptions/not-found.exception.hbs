import {
    NotFoundException as NestJsNotFoundException
} from '@nestjs/common'
import { ApiModelProperty } from '@nestjs/swagger'

export class NotFoundException extends NestJsNotFoundException
{
    @ApiModelProperty({
        description: "A message describing the error",
        default: "Not Found",
        example: "Not Found"
    })
    message : string;

    constructor()
    {
        super({
            message : "Not Found"
        })
    }
}